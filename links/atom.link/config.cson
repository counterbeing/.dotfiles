"*":
  "arduino-upload": {}
  "atom-beautify":
    general:
      _analyticsUserId: "ae612e20-03dc-402d-a8df-30f9064fbf49"
  "atom-ctags":
    autoBuildTagsWhenActive: true
    cmdArgs: "--exclude=node_modules --exclude=bower_components --exclude=./tmp --exclude=.git"
  "command-palette":
    preserveLastSearch: true
  core:
    closeEmptyWindows: false
    disabledPackages: [
      "linter-reek"
      "atom-pair"
      "linter-jshint"
      "language-gfm"
      "markdown-preview"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".tags"
      ".tags1"
      "node_modules"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "copy-path"
    ]
    projectHome: "/Users/cory/Code"
    telemetryConsent: "no"
    themes: [
      "seti-ui"
      "seti-syntax"
    ]
    titleBar: "custom"
  editor:
    fontFamily: "Liga Inconsolata, Meslo"
    fontSize: 17
    lineHeight: 1.3
    showIndentGuide: true
    tabType: "soft"
  "exception-reporting":
    userId: "69e97a2c-a40b-a056-9615-6722c5e60933"
  "find-and-replace":
    liveSearchMinimumCharacters: 4
  "fuzzy-finder":
    ignoredNames: [
      "node_modules/*"
    ]
    preserveLastSearch: true
    searchAllPanes: true
  linter: {}
  "linter-eslint":
    lintHtmlFiles: true
  "linter-gcc":
    execPath: "/usr/local/Cellar/gcc/8.2.0/bin/gcc-8"
    gccDefaultCppFlags: "-c -Wall -std=c++11 -fsyntax-only"
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-reek": {}
  "linter-rubocop":
    command: "/Users/cory/.rbenv/versions/2.3.3/bin/rubocop"
  "linter-sass-lint":
    noConfigDisable: true
  "linter-scss-lint":
    disableWhenNoConfigFileInPath: true
    executablePath: "/Users/cory/.rbenv/versions/2.3.1/bin/scss-lint"
  "linter-ui-default":
    panelHeight: 156
  "markdown-preview-plus": {}
  minimap:
    autoToggle: false
  "one-light-ui":
    layoutMode: "Compact"
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      showInStatusBar: true
    useEslint: true
    useStylelint: true
  "rails-rspec": {}
  "rubocop-auto-correct":
    correctFile: true
    rubocopCommandPath: "/Users/cory/.rbenv/shims/rubocop"
  "seti-syntax":
    themeColor: "Purple"
  "seti-ui":
    compactView: true
    disableAnimations: true
    themeColor: "Green"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  tabs:
    addNewTabsAtEnd: true
  "theme-switch": {}
  "theme-switcher":
    syntaxThemeOne: "seti-syntax"
    syntaxThemeTwo: "one-light-syntax"
    uiThemeOne: "seti-ui"
  "theme-toggler":
    darkSyntaxTheme: "seti-syntax"
    darkUiTheme: "seti-ui"
    lightSyntaxTheme: "one-light-syntax"
    lightUiTheme: "one-light-ui"
  "tree-view":
    alwaysOpenExisting: true
    hideIgnoredNames: true
    squashDirectoryNames: true
  "vim-mode":
    useClipboardAsDefaultRegister: false
  "vim-mode-plus":
    useClipboardAsDefaultRegister: false
  welcome:
    showOnStartup: false
